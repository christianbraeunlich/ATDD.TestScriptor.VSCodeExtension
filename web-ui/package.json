{
  "name": "atdd-web-ui",
  "description": "ATDD.TestScriptor Web UI",
  "version": "0.1.0",
  "repository": {
    "type": "github",
    "url": "https://github.com/fluxxus-nl/ATDD.TestScriptor.VSCodeExtension.git",
    "directory": "web-ui"
  },
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@types/lodash": "^4.14.191",
    "ag-grid-aurelia": "^20.2.0",
    "ag-grid-community": "^20.2.0",
    "aurelia-bootstrapper": "^2.4.0",
    "aurelia-store": "^1.7.2",
    "bootstrap": "^5.2.3",
    "dragula": "^3.7.3",
    "jquery": "^3.6.1",
    "linq": "^4.0.1",
    "lodash": "^4.17.21",
    "requirejs": "^2.3.6",
    "split.js": "^1.6.5",
    "jsonc-parser": "^3.2.0",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@types/jest": "^29.2.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "^14.14.37",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/types": "^7.6.1",
    "aurelia-cli": "^3.0.1",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^1.2.0",
    "aurelia-testing": "^1.1.0",
    "autoprefixer": "^10.4.13",
    "browser-sync": "^2.27.10",
    "connect-history-api-fallback": "^2.0.0",
    "cssnano": "^5.1.14",
    "debounce": "^1.2.1",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-less": "^5.0.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^5.0.0",
    "gulp-watch": "^5.0.1",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jsdom": "^20.0.3",
    "minimatch": "^5.1.1",
    "postcss-url": "^10.1.3",
    "promise-polyfill": "^8.2.3",
    "text": "requirejs/text#latest",
    "through2": "^4.0.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2",
    "vinyl-fs": "3.0.3",
    "eslint": "^7.5.0"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "au build --env prod",
    "start": "au run",
    "test": "au test",
    "docker:build": "docker build -t atdd-web-ui:0.1.0 -t atdd-web-ui:latest .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding",
      "^.+\\.css$": "<rootDir>/test/mock-css"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.(ts|js)$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "globals": {
      "window": {}
    }
  }
}
